  "bugs": {
    "url": "https://github.com/Narapureddy-Srikanth/Recollect/issues"
  },
  "homepage": "https://github.com/Narapureddy-Srikanth/Recollect#readme",
  "dependencies": {
    "ace-builds": "^1.4.12",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "mongoose": "^5.10.9",
    "react": "^17.0.1",
    "react-ace": "^9.2.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.6"
  }
